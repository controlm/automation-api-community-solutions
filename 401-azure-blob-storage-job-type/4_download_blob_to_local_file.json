{
  "Defaults" : {
    "Application" : "AZURE",
    "RunAs" : "<USERNAME>",
    "Host" : "<HOSTNAME>"
  },

  "Azure_Download": {
    "Type": "Folder",
        
    "Azure blob Download job" : {
      "Type" : "Job:ApplicationIntegrator:Azure blob storage",
      "ConnectionProfile" : "AZURE",
        	
      "AI-Action" : "Download",
      "Variables" : [ {
        "UCM-UP_DO_BLOBNAME" : "<AZURE BLOB NAME>"
      }, {
        "UCM-UP_DO_CONTAINER" : "<AZURE BLOB CONTAINER NAME>"
      } ],
      "AI-File path" : "<PATH TO FILE ON AGENT FILE SYSTEM>"
    }
  }
}